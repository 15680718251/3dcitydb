CREATE OR REPLACE
PROCEDURE GetAllConflicts

IS
  status NUMBER;
  message VARCHAR2(256);

BEGIN
  GetAllConflictsBdy(status, message);
  IF status = 1 THEN
    DBMS_OUTPUT.put_line('Gesamtzahl der Konflikte: ' || message);
  ELSE
    DBMS_OUTPUT.put_line('Fehler: ' || message);
  END IF;

END;
/
